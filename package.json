    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/eslint-plugin": "^1.1.4",
    "@vitest/ui": "^2.1.1",
    "babel-loader": "9.2.1",
    "babel-plugin-styled-components": "2.1.4",
    "cypress": "13.15.0",
    "eslint": "9.11.1",
