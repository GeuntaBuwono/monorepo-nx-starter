{
  "name": "monorepo-nx",
  "version": "0.0.0",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": "v20",
    "yarn": "only-pnpm",
    "npm": "only-pnpm"
  },
  "scripts": {
    "env-macos-linux": "export $(cat .env | xargs) && env",
    "preinstall": "npx only-allow pnpm",
    "web:dev": "doppler run -- nx run-many --target=serve --projects=web-*",
    "web:e2e": "doppler run -- nx run-many --target=e2e --projects=web-*",
    "storybook:dev": "doppler run -- nx run storybook-host:storybook",
    "changeset": "changeset",
    "check:lint": "nx run-many --all --target=lint --fix",
    "check:ts": "nx run-many --all --target=tsc",
    "check:test": "nx run-many --all --target=test",
    "checker": "nx run-many --all --target=tsc,lint,test",
    "prepare": "husky",
    "deploy:vercel": "vercel --prod",
    "knip": "knip"
  },
  "private": true,
  "dependencies": {
    "@storybook/addon-interactions": "^8.3.4",
    "next": "14.2.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.13",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@changesets/cli": "^2.27.8",
    "@chromatic-com/storybook": "^2",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "~8.57.0",
    "@nx/cypress": "19.8.2",
    "@nx/eslint": "19.8.2",
    "@nx/eslint-plugin": "19.8.2",
    "@nx/js": "19.8.2",
    "@nx/next": "19.8.2",
    "@nx/react": "19.8.2",
    "@nx/rollup": "19.8.2",
    "@nx/storybook": "19.8.2",
    "@nx/vite": "19.8.2",
    "@nx/web": "19.8.2",
    "@nx/webpack": "19.8.2",
    "@nx/workspace": "19.8.2",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/core-server": "^8.3.4",
    "@storybook/nextjs": "^8.3.4",
    "@storybook/react": "^8.3.4",
    "@svgr/webpack": "8.1.0",
