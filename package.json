{
  "name": "monorepo-nx",
  "version": "0.0.0",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": "v20",
    "yarn": "only-pnpm",
    "npm": "only-pnpm"
  },
  "scripts": {
    "env-macos-linux": "export $(cat .env | xargs) && env",
    "preinstall": "npx only-allow pnpm",
    "web:dev": "doppler run -- nx run-many --target=serve --projects=web-*",
    "web:e2e": "doppler run -- nx run-many --target=e2e --projects=web-*",
    "storybook:dev": "doppler run -- nx run storybook-host:storybook",
    "changeset": "changeset",
    "check:lint": "nx run-many --all --target=lint --fix",
    "check:ts": "nx run-many --all --target=tsc",
    "check:test": "nx run-many --all --target=test",
    "checker": "nx run-many --all --target=tsc,lint,test",
    "prepare": "husky",
    "deploy:vercel": "vercel --prod",
    "knip": "knip"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@storybook/addon-interactions": "^8.3.5",
    "next": "14.2.15",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "@changesets/cli": "^2.27.9",
    "@chromatic-com/storybook": "^2",
    "@emotion/eslint-plugin": "^11.12.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "~9.12.0",
    "@nx/cypress": "19.8.3",
    "@nx/eslint": "19.8.3",
    "@nx/eslint-plugin": "19.8.3",
    "@nx/js": "19.8.3",
    "@nx/next": "19.8.3",
    "@nx/react": "19.8.3",
    "@nx/rollup": "19.8.3",
    "@nx/storybook": "19.8.3",
    "@nx/vite": "19.8.3",
    "@nx/web": "19.8.3",
    "@nx/webpack": "19.8.3",
    "@nx/workspace": "19.8.3",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/core-server": "^8.3.5",
    "@storybook/nextjs": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "16.0.1",
    "@types/jest": "^29.5.13",
    "@types/node": "22.7.5",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/eslint-plugin": "^1.1.7",
    "@vitest/ui": "^2.1.2",
    "babel-loader": "9.2.1",
    "cypress": "13.15.0",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
